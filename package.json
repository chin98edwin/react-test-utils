{
  "name": "@glyph-cat/react-test-utils",
  "version": "0.1.3-experimental.0",
  "description": "Helper functions for writing tests for React with Jest and react-test-renderer.",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "files": [
    "dist",
    "react"
  ],
  "types": "dist/types/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./dist ./temp ./yarn-error.log && mkdir temp dist ",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest ./src",
    "rollup": "rollup -c ./config/rollup.config.js",
    "tsc": "tsc --declaration",
    "api": "api-extractor run -c ./config/api-extractor.json --local --verbose",
    "build": "yarn run rollup && yarn run tsc && yarn run api",
    "all": "yarn clean && yarn lint:fix && yarn build && yarn test && yarn pack"
  },
  "keywords": [
    "React",
    "Test",
    "Jest",
    "React Test Renderer"
  ],
  "author": "glyph-cat",
  "readme": "README.md",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/glyph-cat/react-test-utils.git"
  },
  "bugs": {
    "url": "https://github.com/glyph-cat/react-test-utils/issues"
  },
  "homepage": "https://github.com/glyph-cat/react-test-utils#readme",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-modules-commonjs": "^7.17.9",
    "@babel/plugin-transform-react-jsx": "^7.17.3",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@glyph-cat/eslint-config": "^1.0.0-alpha.0",
    "@microsoft/api-extractor": "^7.23.2",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.32",
    "@types/react": "^18.0.9",
    "@types/react-native": "^0.67.6",
    "@types/react-test-renderer": "^18.0.0",
    "eslint": "^8.15.0",
    "eslint-plugin-functional": "^4.2.1",
    "jest": "^28.1.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-relink": "^3.1.3",
    "react-test-renderer": "^18.1.0",
    "rollup": "^2.72.1",
    "rollup-plugin-typescript2": "^0.31.2",
    "ts-node": "^10.7.0",
    "tsc": "^2.0.4",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@glyph-cat/swiss-army-knife": "^0.0.0-experimental.17"
  }
}
